{
  "name": "guld-sdk-cli",
  "version": "0.1.8",
  "description": "Software Developer Kit for guld JS packages.",
  "bin": {
    "guld-sdk": "cli.js"
  },
  "preferGlobal": true,
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint *.js*",
    "fix": "eslint --fix *.js*",
    "test:lint": "npm run lint && npm run test"
  },
  "keywords": [
    "guld",
    "cli",
    "sdk",
    "node"
  ],
  "author": "isysd",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.17.1",
    "guld-fs": "^1.3.2",
    "guld-user": "^0.5.11"
  },
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.2.1",
    "eslint-plugin-node": "^7.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "pre-commit": "^1.2.2",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "npm-check-updates": "^2.14.2"
  },
  "pre-commit": [
    "test"
  ],
  "readme": "README.md",
  "engines": {
    "node": ">=7"
  },
  "homepage": "https://guld.tech/cli/guld-sdk-cli.html",
  "repository": "https://bitbucket.org/guld/tech-js-node_modules-guld-sdk-cli"
}
