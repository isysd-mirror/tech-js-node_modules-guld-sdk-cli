{
  "name": "guld-random-cli",
  "version": "0.3.0",
  "description": "Cryptographically secure random number generator using `/dev/urandom` with fallback to node's `crypto` and finally to `window.crypto || window.mscrypto`.",
  "bin": {
    "guld-random": "./cli.js"
  },
  "scripts": {
    "lint": "eslint --env browser *.js*",
    "fix": "eslint --fix *.js*"
  },
  "keywords": [
    "guld",
    "git",
    "security",
    "password",
    "cli",
    "encryption"
  ],
  "author": "isysd",
  "license": "MIT",
  "dependencies": {
    "commander": "^2.16.0",
    "guld-random": "^0.3.0"
  },
  "devDependencies": {
    "eslint": "^5.1.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-es5": "^1.3.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "mocha": "^5.2.0",
    "pre-commit": "^1.2.2"
  },
  "pre-commit": [
    "lint"
  ],
  "readme": "README.md",
  "engines": {
    "node": ">=7"
  }
}
